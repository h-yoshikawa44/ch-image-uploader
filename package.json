{
  "name": "ch-image-uploader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint-check": "yarn lint:eslint && yarn check:prettier",
    "lint:eslint": "next lint",
    "check:prettier": "prettier --check \"./{public,src}/**/*.{js,jsx,ts,tsx,html,gql,graphql,json}\"",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn check:prettier --write",
    "preinstall": "typesync || echo 'skip typesync'"
  },
  "dependencies": {
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "18.0.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "typescript": "4.5.5",
    "typesync": "^0.9.0"
  },
  "volta": {
    "node": "14.19.0",
    "yarn": "1.22.17"
  }
}
